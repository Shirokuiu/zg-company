body {
  font-size: 16px;
  line-height: 19px;
  font-weight 400
  font-family: 'Montserrat', sans-serif;
  color: #000000;
  font-style: normal;
}

*::-webkit-input-placeholder {
  color grayLowLight
  font-size: 18px;
  line-height: 21px;
  font-weight 300
  
  opacity: 1;
}

